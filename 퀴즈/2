Quiz 10
    var name = "javarouka";
    function introduce() {
        var intro = "My name is ";
    
        if(!name) {
            var name = "unknown"
        }
        return intro += name;
    }
    alert(introduce());

    1."My name is undefined"
    2."My name is null"
    3."My name is javarouka"
    4."My name is unknown"